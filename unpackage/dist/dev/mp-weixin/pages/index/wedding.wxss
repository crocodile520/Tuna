
.dynamic-container.data-v-8ae5a4b6 {
		display: flex;
		flex-direction: row;
		flex-wrap: wrap;
		width: 100%;
		background-color: #f5f5f5;
}
.dynamic-item.data-v-8ae5a4b6 {
		width: 46%;
		height: 500rpx;
		border-radius: 8px;
		/* 圆角 */
		background-color: #FFFFFF;
		margin: 2% 2%;
}
.movable-area.data-v-8ae5a4b6 {
		width: 100vw;
		height: 100vh;
		position: relative;
}
.movable-view.data-v-8ae5a4b6 {
		width: 100%;
		height: 100%;
		display: flex;
		justify-content: center;
		align-items: center;
		transition: all 0.3s ease;
		/* 添加过渡效果 */
}
.popup-content.data-v-8ae5a4b6 {
		width: 100vw;
		height: 100vh;
		background-color: rgba(0, 0, 0, 0.8);
}
.img.data-v-8ae5a4b6 {
		width: 100%;
		height: 100%;
		transition: transform 0.3s ease;
		/* 添加图片变换过渡效果 */
}
